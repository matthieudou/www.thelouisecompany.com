{
  "name": "thelouisecompany.com",
  "version": "1.0.0",
  "description": "Website for The Louise Company",
  "author": "matthieudou <matthieu.doultremont@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "yarn nuxt:dev",
    "nuxt:dev": "nuxt",
    "sanity:dev": "cd sanity && sanity start",
    "functions:dev": "yarn functions:serve",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "functions:serve": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions",
    "production:build": "yarn generate && yarn functions:build",
    "svg": "vsvg -s ./assets/images/icons/svg -t ./icons --es6"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.2",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/sentry": "^3.0.0",
    "@nuxtjs/sitemap": "^2.0.1",
    "@sanity/client": "^0.142.0",
    "@sanity/image-url": "^0.140.12",
    "@sendgrid/mail": "^6.4.0",
    "dotenv": "^8.0.0",
    "groq": "^1.150.1",
    "gsap": "^3.6.0",
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.11",
    "netlify-lambda": "^1.6.3",
    "nuxt": "^2.14.4",
    "nuxt-i18n": "^6.0.2",
    "v-lazy-image": "^1.3.2",
    "vue-mq": "^1.0.1",
    "vue-scrollto": "^2.15.0",
    "vue2-touch-events": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/google-analytics": "^2.2.1",
